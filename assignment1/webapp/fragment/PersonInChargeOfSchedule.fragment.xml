<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog
		id="selectDialog2"
		title="{i18n>schedulePerson}"
		searchPlaceholder="{i18n>search}"
		items="{data>/Collection}"
		search=".onPersonInChargeOfScheduleHelpSearch"
		confirm=".onPersonInChargeOfScheduleHelpClose"
		cancel=".onPersonInChargeOfScheduleHelpClose">
		<StandardListItem
			iconDensityAware="false"
			iconInset="false"
			title="{data>PersonInChargeOfSchedule/code}"
			description="{data>PersonInChargeOfSchedule/name}"
		 />
	</SelectDialog>
</core:FragmentDefinition>