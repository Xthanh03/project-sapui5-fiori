<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog
		id="selectDialog1"
		title="{i18n>vendor}"
		searchPlaceholder="{i18n>search}"
		items="{data>/Collection}"
		search=".onValueVendorHelpSearch"
		confirm=".onValueVendorHelpClose"
		cancel=".onValueVendorHelpClose">
		<StandardListItem
			iconDensityAware="false"
			iconInset="false"
			title="{data>Vendor/code}"
			description="{data>Vendor/name}"
		 />
	</SelectDialog>
</core:FragmentDefinition>